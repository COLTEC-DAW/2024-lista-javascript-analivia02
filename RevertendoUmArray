function reverseArray(array) {
    let reverse = '';
    for (let i = array.length - 1; i >= 0; i--) {
        reverse += array[i];
    }
    return reverse;
}
let frase = prompt("Digite a frase que deseja inverter:");

let fraseInvertida = reverseArray(frase);

console.log("Frase invertida:", fraseInvertida);

